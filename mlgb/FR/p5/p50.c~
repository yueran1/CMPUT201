#include<string>
#include<iostream>
using namespace std;
int main()
{
  string s;
  string result;
  cin>>s;
  for(unsigned int i=0; i<s.size();i++)
  {
    if(s[i]=='a')
    {
     result=result+ s[i]='1';
    }
    if(s[i]=='b')
    {
      result=result+ s[i]='2';
    }
    if(s[i]=='c')
    {
     result=result+  s[i]='3';
    }
    if(s[i]=='d')
    {
     result=result+  s[i]='4';
    }
    if(s[i]=='e')
    {
      result=result+ s[i]='5';
    }
    if(s[i]=='f')
    {
      result=result+ s[i]='6';
    }
    if(s[i]=='g')
    {
       result=result+s[i]='7';
    }
    if(s[i]=='h')
    {
      result=result+ s[i]='8';
    }
    if(s[i]=='i')
    {
      result=result+ s[i]='9';
    }
    if(s[i]=='j')
    {
      result=result+ s[i]='0';
    }
    if(s[i]=='k')
    {
      result=result+ s[i]='1';
    }
    if(s[i]=='l')
    {
      result=result+ s[i]='2';
    }
    if(s[i]=='m')
    {
      result=result+ s[i]='3';
    }
    if(s[i]=='n')
    {
      result=result+ s[i]='4';
    }
    if(s[i]=='o')
    {
      result=result+ s[i]='5';
    }
    if(s[i]=='p')
    {
      result=result+ s[i]='6';
    }
    if(s[i]=='q')
    {
      result=result+ s[i]='7';
    }
    if(s[i]=='r')
    {
      result=result+ s[i]='8';
    }
    if(s[i]=='s')
    {
      result=result+ s[i]='9';
    }
    if(s[i]=='t')
    {
      result=result+ s[i]='0';
    }
    if(s[i]=='u')
    {
      result=result+ s[i]='1';
    }
    if(s[i]=='v')
    {
      result=result+ s[i]='2';
    }
    if(s[i]=='w')
    {
      result=result+ s[i]='3';
    }
    if(s[i]=='x')
    {
      result=result+ s[i]='4';
    }
    if(s[i]=='y')
    {
      result=result+ s[i]='5';
    }
    if(s[i]=='z')
    {
      result=result+ s[i]='6';
    }
    else
    {
      result-result+s[i];
    }
    
  }
  cout<<result<<endl;

}
